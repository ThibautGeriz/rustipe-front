{
  "expo": {
    "name": "rustipe",
    "slug": "rustipe",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.tgery.rustipe",
      "buildNumber": "0.0.1",
      "supportsTablet": true
    },
    "android": {
      "package": "org.tgery.rustipe",
      "versionCode": 1
    }
  }
}
